module stalling_unit

	(
		input					EX.MemRead,
		input					MEM.MemRead,
		input		[4:0]		EX.rd,
		input		[4:0]		MEM.rd,
		input		[4:0]		ID.rs1,
		input		[4:0]		ID.rs2
		output				MemStall
	);
	
	assign MemStall = (EX.MemRead & (EX.rd == ID.rs1 | EX.rd == ID.rs2))|(MEM.MemRead & (MEM.rd == ID.rs1 | MEM.rd == ID.rs2)) 